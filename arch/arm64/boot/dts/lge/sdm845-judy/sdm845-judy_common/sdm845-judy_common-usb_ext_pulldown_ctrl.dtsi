&tlmm {
	atest_usb_active: atest_usb_active {
		mux {
			pins = "gpio8";
			function = "gpio";
		};
		config {
			pins = "gpio8";
			drive-strength = <12>;
			bias-pull-up;
		};
	};
	atest_usb_suspend: atest_usb_suspend {
		mux {
			pins = "gpio8";
			function = "gpio";
		};
		config {
			pins = "gpio8";
			bias-pull-down;
			drive-strength = <2>;
		};
	};
};

&qusb_phy0 {
	pinctrl-names = "atest_usb_suspend", "atest_usb_active";
	pinctrl-0 = <&atest_usb_suspend>;
	pinctrl-1 = <&atest_usb_active>;
};
